<?xml version="1.0" encoding="utf-8"?>

<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
	
	<!-- Archotech Hediff Colour -->
			
			<!-- Archotech Eye -->
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/HediffDef[defName = "ArchotechEye"]</xpath>
				<value>
					<defaultLabelColor>(139,143,104)</defaultLabelColor>
				</value>
			</li>
			
			<!-- Archotech Arm -->
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/HediffDef[defName = "ArchotechArm"]</xpath>
				<value>
					<defaultLabelColor>(139,143,104)</defaultLabelColor>
				</value>
			</li>
			
			<!-- Archotech Leg -->
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/HediffDef[defName = "ArchotechLeg"]</xpath>
				<value>
					<defaultLabelColor>(139,143,104)</defaultLabelColor>
				</value>
			</li>
	
		</operations>
	</Operation>
	
</Patch>