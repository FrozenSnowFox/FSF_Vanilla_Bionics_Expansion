<?xml version="1.0" encoding="utf-8"?>

<Patch>
	
  	<!-- Hediffs_BodyParts_Archotech.xml -->
	
	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>

			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[@Name = "BodyPartArchotechBase"]/graphicData/color</xpath>
				<value>
					<color>(139,143,104)</color>
				</value>
			</li>
			
			<!-- Big And Small Genes & More - Archotech Apparel-Integrator -->
			
			<li MayRequire="RedMattis.BigSmall.Core" Class="PatchOperationAdd">
				<xpath>/Defs/HediffDef[defName = "BS_IndestructibelApparel"]</xpath>
				<value>
					<defaultLabelColor>(139,143,104)</defaultLabelColor>
				</value>
			</li>
			
		</operations>
	</Operation>
	
</Patch>