<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	
	<!-- Artificial Nose -->
	
	<HediffDef ParentName="FSFAddedBodyPartProsthetic">
		<defName>FSFArtificialNose</defName>
		<label>artificial nose</label>
		<labelNoun>a artificial nose</labelNoun>
		<description>An installed artificial nose.</description>
		<descriptionHyperlinks><ThingDef>FSFArtificialNose</ThingDef></descriptionHyperlinks>
		<spawnThingOnRemoved>FSFArtificialNose</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
		</addedPartProps>
	</HediffDef>
	
	<ThingDef ParentName="BodyPartProstheticMakeableBase">
		<defName>FSFArtificialNose</defName>
		<label>artificial nose</label>
		<description>An artificial nose that works as a suitable replacement for an organic one.</description>
		<descriptionHyperlinks><RecipeDef>InstallFSFArtificialNose</RecipeDef></descriptionHyperlinks>
		<costList>
			<Steel>10</Steel>
		</costList>
		<butcherProducts>
			<Steel>10</Steel>
		</butcherProducts>
		<statBases>
			<Mass>0.05</Mass>
		</statBases>
		<techHediffsTags>
			<li>Simple</li>
		</techHediffsTags>
	</ThingDef>
	
	<RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
		<defName>InstallFSFArtificialNose</defName>
		<label>install artificial nose</label>
		<description>Install a artificial nose.</description>
		<descriptionHyperlinks>
			<ThingDef>FSFArtificialNose</ThingDef>
			<HediffDef>FSFArtificialNose</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing artificial nose.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>FSFArtificialNose</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>FSFArtificialNose</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Nose</li>
		</appliedOnFixedBodyParts>
		<addsHediff>FSFArtificialNose</addsHediff>
		<uiIconThing>FSFArtificialNose</uiIconThing>
	</RecipeDef>
	
	<!-- Artificial Bone -->
	
	<HediffDef ParentName="FSFAddedBodyPartProsthetic">
		<defName>FSFArtificialBone</defName>
		<label>artificial bone</label>
		<labelNoun>a artificial bone</labelNoun>
		<description>An installed artificial bone.  An artificial bone that works as a suitable replacement for an organic one.</description>
		<descriptionHyperlinks><ThingDef>FSFArtificialBone</ThingDef></descriptionHyperlinks>
		<spawnThingOnRemoved>FSFArtificialBone</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
		</addedPartProps>
	</HediffDef>
	
	<ThingDef ParentName="BodyPartProstheticMakeableBase">
		<defName>FSFArtificialBone</defName>
		<label>artificial bone</label>
		<description>An artificial bone that works as a suitable replacement for an organic one.</description>
		<descriptionHyperlinks><RecipeDef>InstallFSFArtificialBone</RecipeDef></descriptionHyperlinks>
		<costList>
			<Steel>20</Steel>
		</costList>
		<butcherProducts>
			<Steel>20</Steel>
		</butcherProducts>
		<statBases>
			<Mass>1</Mass>
		</statBases>
		<techHediffsTags>
			<li>Simple</li>
		</techHediffsTags>
	</ThingDef>
	
	<RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
		<defName>InstallFSFArtificialBone</defName>
		<label>install artificial bone</label>
		<description>Install a artificial bone.</description>
		<descriptionHyperlinks>
			<ThingDef>FSFArtificialBone</ThingDef>
			<HediffDef>FSFArtificialBone</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing artificial bone.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>FSFArtificialBone</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>FSFArtificialBone</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Femur</li>
			<li>Humerus</li>
			<li>Radius</li>
			<li>Tibia</li>
		</appliedOnFixedBodyParts>
		<addsHediff>FSFArtificialBone</addsHediff>
		<uiIconThing>FSFArtificialBone</uiIconThing>
	</RecipeDef>
	
</Defs>