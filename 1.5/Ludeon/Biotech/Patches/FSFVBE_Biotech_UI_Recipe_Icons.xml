<?xml version="1.0" encoding="utf-8"?>

<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
	
	<!-- Add Bionic Icons to Biotech Items -->
			
			<!-- Detoxifier Lung -->
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "InstallDetoxifierLung"]</xpath>
				<value>
					<uiIconThing>DetoxifierLung</uiIconThing>
				</value>
			</li>
			
			<!-- Detoxifier Kidney -->
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "InstallDetoxifierKidney"]</xpath>
				<value>
					<uiIconThing>DetoxifierKidney</uiIconThing>
				</value>
			</li>
	
		</operations>
	</Operation>
	
</Patch>