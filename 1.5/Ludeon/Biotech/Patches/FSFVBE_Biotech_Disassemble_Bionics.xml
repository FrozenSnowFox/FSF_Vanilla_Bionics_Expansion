<?xml version="1.0" encoding="utf-8"?>

<Patch>
	
  	<!-- Add Butchery Products To Bionics -->
	
	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			
  			<!-- Biotech Bionics -->
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "DetoxifierLung"]</xpath>
				<value>
					<butcherProducts>
						<Plasteel>15</Plasteel>
						<ComponentSpacer>4</ComponentSpacer>
					</butcherProducts>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "DetoxifierKidney"]</xpath>
				<value>
					<butcherProducts>
						<Plasteel>15</Plasteel>
						<ComponentSpacer>2</ComponentSpacer>
					</butcherProducts>
				</value>
			</li>
			
  			<!-- Biotech Mechtech -->
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "ControlSublink"]</xpath>
				<value>
					<butcherProducts>
						<SignalChip>1</SignalChip>
						<ComponentIndustrial>4</ComponentIndustrial>
					</butcherProducts>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "ControlSublinkHigh"]</xpath>
				<value>
					<butcherProducts>
						<PowerfocusChip>1</PowerfocusChip>
						<ComponentIndustrial>3</ComponentIndustrial>
						<ComponentSpacer>3</ComponentSpacer>
					</butcherProducts>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "RemoteRepairer"]</xpath>
				<value>
					<butcherProducts>
						<PowerfocusChip>1</PowerfocusChip>
						<ComponentIndustrial>6</ComponentIndustrial>
					</butcherProducts>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "MechFormfeeder"]</xpath>
				<value>
					<butcherProducts>
						<SignalChip>1</SignalChip>
						<ComponentIndustrial>6</ComponentIndustrial>
					</butcherProducts>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "RemoteShielder"]</xpath>
				<value>
					<butcherProducts>
						<PowerfocusChip>1</PowerfocusChip>
						<ComponentIndustrial>6</ComponentIndustrial>
					</butcherProducts>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "RepairProbe"]</xpath>
				<value>
					<butcherProducts>
						<NanostructuringChip>1</NanostructuringChip>
						<ComponentSpacer>2</ComponentSpacer>
					</butcherProducts>
				</value>
			</li>
			
		</operations>
	</Operation>
	
</Patch>