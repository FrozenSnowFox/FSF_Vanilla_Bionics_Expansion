<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<!-- Bionics Skill -->

	<VSE.Expertise.ExpertiseDef>
		<defName>FSFBionics</defName>
		<label>Bionics</label>
		<skill>Crafting</skill>
		<description>An expertise in crafting bionics and bionics related items efficiently.</description>
		<statOffsets>
			<FSFBionicsCraftingSpeed>0.05</FSFBionicsCraftingSpeed>
		</statOffsets>
	</VSE.Expertise.ExpertiseDef>
	
    <StatDef>
      <defName>FSFBionicsCraftingSpeed</defName>
      <label>bionics crafting speed</label>
