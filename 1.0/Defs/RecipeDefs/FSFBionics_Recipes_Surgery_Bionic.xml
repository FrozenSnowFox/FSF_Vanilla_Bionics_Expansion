	<RecipeDef ParentName="FSFBionicSurgery">
		<defName>InstallFSFBionicPowerArm</defName>
		<label>install bionic power arm</label>
		<description>Install a bionic power arm.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing bionic power arm.</jobString>
		<workAmount>2500</workAmount>
		<skillRequirements>
			<Medicine>5</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>FSFBionicPowerArm</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>FSFBionicPowerArm</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Shoulder</li>
		</appliedOnFixedBodyParts>
		<addsHediff>FSFBionicPowerArm</addsHediff>
	</RecipeDef>
	
