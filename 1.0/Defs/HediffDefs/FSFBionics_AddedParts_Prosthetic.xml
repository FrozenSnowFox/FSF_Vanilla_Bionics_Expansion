<?xml version="1.0" encoding="utf-8" ?>

<Defs>

  <HediffDef Name="FSFAddedBodyPartProsthetic" Abstract="True">
    <hediffClass>Hediff_AddedPart</hediffClass>
	<defaultLabelColor>(102,178,255)</defaultLabelColor>
    <isBad>false</isBad>
  </HediffDef>

	<!-- Vanilla Prosthetic Body Parts -->

  <HediffDef ParentName="FSFAddedBodyPartProsthetic">
    <defName>Denture</defName>
    <label>denture</label>
    <labelNoun>a denture</labelNoun>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.80</partEfficiency>
      <betterThanNatural>false</betterThanNatural>
    </addedPartProps>
  </HediffDef>

  <HediffDef ParentName="FSFAddedBodyPartProsthetic">
    <defName>SimpleProstheticLeg</defName>
