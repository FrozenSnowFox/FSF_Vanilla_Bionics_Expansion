<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	
	<!-- Bionic Jaw -->
	
	<HediffDef ParentName="AddedBodyPartBase">
		<defName>FSFBionicJaw</defName>
		<label>bionic jaw</label>
		<labelNoun>a bionic jaw</labelNoun>
		<description>An installed bionic jaw.</description>
		<descriptionHyperlinks><ThingDef>FSFBionicJaw</ThingDef></descriptionHyperlinks>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>teeth</label>
						<capacities>
							<li>Bite</li>
						</capacities>
						<power>9.2</power>
						<cooldownTime>2</cooldownTime>
						<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
						<chanceFactor>0.07</chanceFactor>
					</li>
				</tools>
			</li>
		</comps>
		<spawnThingOnRemoved>FSFBionicJaw</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.25</partEfficiency>
			<betterThanNatural>true</betterThanNatural>
		</addedPartProps>
	</HediffDef>
	
	<ThingDef ParentName="BodyPartBionicBase">
		<defName>FSFBionicJaw</defName>
		<label>bionic jaw</label>
		<description>An advanced artificial jaw. Silenced mini-servos give great strength, while a lattice-dust healing system allows it to recover from damage. It is better than a biological jaw in almost every way.</description>
		<descriptionHyperlinks><RecipeDef>InstallFSFBionicJaw</RecipeDef></descriptionHyperlinks>
		<costList>
			<Plasteel>10</Plasteel>
			<ComponentSpacer>3</ComponentSpacer>
		</costList>
		<thingSetMakerTags><li>RewardStandardMidFreq</li></thingSetMakerTags>
		<statBases>
			<Mass>1.5</Mass>
		</statBases>
	</ThingDef>
	
	<RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
		<defName>InstallFSFBionicJaw</defName>
		<label>install bionic jaw</label>
		<description>Install a bionic jaw.</description>
		<descriptionHyperlinks><ThingDef>FSFBionicJaw</ThingDef></descriptionHyperlinks>
		<jobString>Installing bionic jaw.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>FSFBionicJaw</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>FSFBionicJaw</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Jaw</li>
		</appliedOnFixedBodyParts>
		<addsHediff>FSFBionicJaw</addsHediff>
	</RecipeDef>
	
